    obo:IAO_0000115 "Data is analyzed within multiple study experimental units."^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    rdfs:subClassOf :OCRE100046 ;
    owl:disjointWith :OCRE100085 ;
    skos:prefLabel "Data analyzed within multiple experimental units"^^xsd:string .

:OCRE100066 a owl:Class ;
    rdfs:label "OCRe Phase 4" ;
    obo:IAO_0000115 "A Phase 4 study monitors FDA-approved drug to delineate additional information including the drug's risks, benefits, and optimal use. "^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    obo:IAO_0000119 "ClinicalTrials.gov Protocol Data Element Definitions (DRAFT). Available at  http://prsinfo.clinicaltrials.gov/definitions.html"^^xsd:string ;
    rdfs:subClassOf :OCRE100038 ;
    skos:prefLabel "Phase 4"^^xsd:string .

:OCRE100067 a owl:Class ;
    rdfs:label "OCRe Phase 1" ;
    obo:IAO_0000115 "A Phase 1 trial includes initial studies to determine the metabolism and pharmacologic actions of drugs in humans, the side effects associated with increasing doses, and to gain early evidence of effectiveness; may include healthy participants and/or patients."^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    obo:IAO_0000119 "ClinicalTrials.gov Protocol Data Element Definitions (DRAFT). Available at  http://prsinfo.clinicaltrials.gov/definitions.html"^^xsd:string ;
    rdfs:subClassOf :OCRE100038 ;
    skos:prefLabel "Phase 1"^^xsd:string .

:OCRE100068 a owl:Class ;
    rdfs:label "OCRe Phase 0" ;
    obo:IAO_0000115 "A Phase 0 trial is an exploratory trial involving very limited human exposure, with no therapeutic or diagnostic intent (e.g., screening study, microdose study)."^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    obo:IAO_0000119 "ClinicalTrials.gov Protocol Data Element Definitions (DRAFT). Available at  http://prsinfo.clinicaltrials.gov/definitions.html"^^xsd:string ;
    rdfs:subClassOf :OCRE100038 ;
    skos:prefLabel "Phase 0"^^xsd:string .

:OCRE100069 a owl:Class ;
    rdfs:label "OCRe Phase 3" ;
    obo:IAO_0000115 "A Phase 3 trial includes expanded controlled and uncontrolled trials after preliminary evidence suggesting effectiveness of the drug has been obtained, and are intended to gather additional information to evaluate the overall benefit-risk relationship of the drug and provide an adequate basis for physician labeling."^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    obo:IAO_0000119 "ClinicalTrials.gov Protocol Data Element Definitions (DRAFT). Available at  http://prsinfo.clinicaltrials.gov/definitions.html"^^xsd:string ;
    rdfs:subClassOf :OCRE100038 ;
