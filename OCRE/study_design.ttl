:OCRE100006 a owl:Class ;
    rdfs:label "OCRe Probability sampling" ;
    obo:IAO_0000115 "Probability sampling is an exclusively random process to guarantee that each participant or population has specified chance of selection, such as simple random sampling, systematic sampling, stratified random sampling, cluster sampling, and consecutive patient sampling"^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    obo:IAO_0000119 "ClinicalTrials.gov Protocol Data Element Definitions (DRAFT). Available at  http://prsinfo.clinicaltrials.gov/definitions.html"^^xsd:string ;
    rdfs:subClassOf :OCRE100049 ;
    owl:disjointWith :OCRE100019 ;
    skos:prefLabel "Probability sampling"^^xsd:string .

:OCRE100009 a owl:Class ;
    rdfs:label "OCRe Investigator assigns no intervention" ;
    obo:IAO_0000115 "The investigator does not actively assign study experimental units to regimens of interventions."^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    owl:disjointWith :OCRE100026 ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( :OCRE100058 [ a owl:Class ;
                        owl:complementOf :OCRE100026 ] ) ] ;
    skos:prefLabel "Investigator assigns no intervention"^^xsd:string .

:OCRE100012 a owl:Class ;
    rdfs:label "OCRe Equivalence" ;
    obo:IAO_0000115 """Equivalence is a type of comparative intent. 
An equivalence trial is designed to determine whether the response to two or more treatments differs by an amount that is clinically unimportant. This is usually demonstrated by showing that the true treatment difference is likely to lie between a lower and an upper equivalence level of clinically acceptable differences. """^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    obo:IAO_0000119 "Glossary in The Cochrane Collaboration. Available at http://www.cochrane.org/glossary"^^xsd:string ;
    rdfs:subClassOf :OCRE100072 ;
    owl:disjointWith :OCRE100076 ;
    skos:prefLabel "Equivalence"^^xsd:string .

:OCRE100014 a owl:Class ;
    rdfs:label "OCRe Outcome assessor blinded" ;
    obo:IAO_0000115 "The outcome assessor does not know to which treatment group a particular participant belongs "^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    rdfs:subClassOf :OCRE100045 ;
    skos:prefLabel "Outcome assessor blinded"^^xsd:string .

:OCRE100018 a owl:Class ;
    rdfs:label "OCRe Case-control study design" ;
    obo:IAO_0000115 "A case control study is an observational study that compares people with a specific disease or outcome of interest (cases) to people from the same population without that disease or outcome (controls), and which seeks to find associations between the outcome and prior exposure to particular risk factors. [Glossary of Terms in The Cochrane Collaboration]"^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    owl:disjointWith :OCRE100039,
        :OCRE100078 ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( :OCRE100055 [ a owl:Restriction ;
                        owl:onProperty :OCRE900001 ;
                        owl:someValuesFrom :OCRE100079 ] [ a owl:Restriction ;
                        owl:onProperty :OCRE900001 ;
                        owl:someValuesFrom :OCRE100088 ] ) ] ;
    skos:prefLabel "Case-control study design"^^xsd:string .

:OCRE100019 a owl:Class ;
    rdfs:label "OCRe Non-probability sampling" ;
    obo:IAO_0000115 "Non-probability sampling is any one of a variety of non random sampling processes, such as convenience sampling or invitation to volunteer"^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    obo:IAO_0000119 "ClinicalTrials.gov Protocol Data Element Definitions (DRAFT). Available at  http://prsinfo.clinicaltrials.gov/definitions.html"^^xsd:string ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( :OCRE100049 [ a owl:Class ;
                        owl:complementOf :OCRE100006 ] ) ] ;
    skos:prefLabel "Non-probability sampling"^^xsd:string .

:OCRE100024 a owl:Class ;
    rdfs:label "OCRe N-of-1 crossover study design" ;
    obo:IAO_0000115 "An N-of-1 crossover study is an interventional study where the population is a single human subject and in which at least two regimens of interventions are given alternatingly. Regimens are assigned to and data are analyzed within one participant at a time."^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    obo:IAO_0000232 "Simona: to be reviewed"^^xsd:string ;
    owl:disjointWith :OCRE100027,
        :OCRE100052 ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( :OCRE100007 [ a owl:Restriction ;
                        owl:onProperty :OCRE900001 ;
                        owl:someValuesFrom :OCRE100035 ] [ a owl:Restriction ;
                        owl:onProperty :OCRE900001 ;
                        owl:someValuesFrom :OCRE100075 ] [ a owl:Restriction ;
                        owl:onProperty :OCRE900001 ;
                        owl:someValuesFrom :OCRE100081 ] [ a owl:Restriction ;
                        owl:onProperty :OCRE900001 ;
                        owl:someValuesFrom :OCRE100085 ] ) ] ;
    skos:prefLabel "N-of-1 crossover study design"^^xsd:string .

:OCRE100029 a owl:Class ;
    rdfs:label "OCRe Multiple experimental unit allocation group" ;
    obo:IAO_0000115 "In the context of a study where the main comparison is within experimental units, investigator assigns regimen of interventions to an allocation group made up of more than one experimental unit."^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    rdfs:subClassOf :OCRE100002 ;
    owl:disjointWith :OCRE100075 ;
    skos:prefLabel "Multiple experimental unit allocation group"^^xsd:string .

:OCRE100031 a owl:Class ;
    rdfs:label "OCRe Quantitative inquiry mode" ;
    obo:IAO_0000115 "A quantitative research method is a deductive research process that uses statistical techniques to test a specific claim or statement about a phenomenon (a hypothesis) by collecting and analyzing data to decide whether the claim is true (i.e., accept or reject the null or alternative hypothesis)."^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    obo:IAO_0000119 "http://www.utexas.edu/academic/ctl/assessment/iar/research/plan/types.php"^^xsd:string ;
    rdfs:subClassOf :OCRE100028 ;
    owl:disjointWith :OCRE100057 ;
    skos:prefLabel "Quantitative inquiry mode"^^xsd:string .

:OCRE100032 a owl:Class ;
    rdfs:label "OCRe Quasi-random allocation" ;
    obo:IAO_0000115 "Quasi-random allocation is an allocation scheme that is not random, but is intended to produce similar groups when used to allocate experimental units. Quasi-random methods include: allocation by the person's date of birth, by the day of the week or month of the year, by a person's medical record number, or just allocating every alternate person. In practice, these methods of allocation are relatively easy to manipulate, introducing selection bias."^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    obo:IAO_0000119 "Glossary in The Cochrane Collaboration. Available at http://www.cochrane.org/glossary"^^xsd:string ;
    rdfs:subClassOf :OCRE100021 ;
    owl:disjointWith :OCRE100033,
        :OCRE100077 ;
    skos:prefLabel "Quasi-random allocation"^^xsd:string .

:OCRE100034 a owl:Class ;
    rdfs:label "OCRe Genetically related non-cases" ;
    obo:IAO_0000115 "Non-cases were selected based on genetic relation"^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    rdfs:subClassOf :OCRE100082 ;
    owl:disjointWith :OCRE100086 ;
    skos:prefLabel "Genetically related non-cases"^^xsd:string .

:OCRE100036 a owl:Class ;
    rdfs:label "OCRe Caregiver blinded" ;
    obo:IAO_0000115 "The caregiver does not know to which treatment group a particular participant belongs "^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    rdfs:subClassOf :OCRE100045 ;
    skos:prefLabel "Caregiver blinded"^^xsd:string .

:OCRE100053 a owl:Class ;
    rdfs:label "OCRe Care provider blinded" ;
    obo:IAO_0000115 "The care provider does not know to which treatment group a particular participant belongs"^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    rdfs:subClassOf :OCRE100045 ;
    skos:prefLabel "Care provider blinded"^^xsd:string .

:OCRE100061 a owl:Class ;
    rdfs:label "OCRe Main comparison across experimental units" ;
    obo:IAO_0000115 "The main comparison is across study experimental units."^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    rdfs:subClassOf :OCRE100073 ;
    owl:disjointWith :OCRE100081 ;
    skos:prefLabel "Main comparison across experimental units"^^xsd:string .

:OCRE100062 a owl:Class ;
    rdfs:label "OCRe Non-nested" ;
    obo:IAO_0000115 """Cases and controls (or exposed and unexposed) are not drawn from the same cohort/sample.
 [aka double cohort for cohort studies]"""^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
