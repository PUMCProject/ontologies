:OCRE100008 a owl:Class ;
    rdfs:label "OCRe Open label" ;
    obo:IAO_0000115 "Open label indicates that no blinding is in effect: the investigator and participant are aware which intervention is being used for which participant (i.e. not blinded)"^^xsd:string ;
    obo:IAO_0000117 "Simona"^^xsd:string ;
    obo:IAO_0000119 "Glossary in The Cochrane Collaboration. Available at http://www.cochrane.org/glossary"^^xsd:string ;
    owl:disjointWith :OCRE100014,
        :OCRE100036,
        :OCRE100053,
        :OCRE100064,
